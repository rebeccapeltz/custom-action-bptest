{
  "app_id": "custom-action-bptest",
  "app_name": "Custom Action Testing",
  "app_url": "https://www.beckypeltz.me/custom-action-bptest/",
  "app_role": [
    "media_library_user",
    "media_library_admin",
    "admin",
    "master_admin"
  ],
  "app_message_params": {
    "cloudName": {
      "type": "cloud_name"
    },
    "assets": {
      "type": "selected_items",
      "attributes": [
        "public_id",
        "resource_type",
        "tags",
        "context"
      ]
    }
  },
  "app_permissions": {
    "asset": [
      "update:tags",
      "update:transformation",
      "download",
      "upload"
    ],
    "collection": [
      "create",
      "get",
      "update:assets"
    ]
  },
  "app_active": true,
  "app_open_in": "modal",
  "app_supporeted_devices": [
    "desktop",
    "tablet",
    "mobile"
  ],
  "action_label": "BP Test",
  "action_showinpage": [
    "folders"
  ],
  "action_icon": "https://res.cloudinary.com/prod/raw/upload/console/custom-actions/custom-action-bptest/manifest.json",
  "asset_type": [
    "image"
  ],
  "selection_mode": [
    "multiple"
  ],
  "selection_type": [
    "asset"
  ]
}